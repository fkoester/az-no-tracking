{
    "manifest_version": 2,
    "name": "VRM Ad-Blocker-Warnung ausblenden",
    "version": "1.3",
    "description": "Blendet auf Nachrichtenportalen der VRM-Gruppe die \"Bitte deaktivieren Sie Ihren Ad-Blocker\"-Warnung aus.",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.allgemeine-zeitung.de/*",
                "*://*.buerstaedter-zeitung.de/*",
                "*://*.echo-online.de/*",
                "*://*.hochheimer-zeitung.de/*",
                "*://*.lampertheimer-zeitung.de/*",
                "*://*.lauterbacher-anzeiger.de/*",
                "*://*.main-spitze.de/*",
                "*://*.mittelhessen.de/*",
                "*://*.oberhessische-zeitung.de/*",
                "*://*.wiesbadener-kurier.de/*",
                "*://*.wiesbadener-tagblatt.de/*",
                "*://*.wormser-zeitung.de/*"
            ],
            "js": ["az-no-tracking.js"]
        }
    ]

}
